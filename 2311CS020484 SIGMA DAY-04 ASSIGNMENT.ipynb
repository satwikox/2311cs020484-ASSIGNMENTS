   "source": [
    "def sum_of_evens(n):\n",
    "    total_sum = 0\n",
    "    for i in range(1, n+1):\n",
    "        if i % 2 == 0:\n",
    "            total_sum += i\n",
    "    return total_sum\n",
    "\n",
    "n = int(input(\"Enter a positive integer: \"))\n",
    "print(\"The sum of all even numbers is:\", sum_of_evens(n))\n"
   ]
